@model IEnumerable<RayaHesab.Models.Mastermenutb>

@{
    ViewData["Title"] = "Index";
    Layout = Context.Request.Cookies["layoutmain"];

}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Title)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Title)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Mid">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Mid">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Mid">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
